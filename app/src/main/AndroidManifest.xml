<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CafeMobileAplication"
        tools:targetApi="31">
        <activity
            android:name=".MainActivityCart"
            android:exported="true"
            android:parentActivityName=".MainActivityProduct"/>
        <activity
            android:name=".MainActivityProduct"
            android:exported="true"
            android:parentActivityName=".MainActivityCakeMenu"/>
        <activity
            android:name=".MainActivityGoToMenu"
            android:exported="true"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivityCakeMenu"
            android:exported="true"
            android:parentActivityName=".MainActivityCoffeeMenu"/>
        <activity
            android:name=".MainActivityCoffeeMenu"
            android:exported="true"
            android:parentActivityName=".MainActivityMenu"/>
        <activity
            android:name=".MainActivityMenu"
            android:exported="true"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivityNewUser"
            android:exported="true"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>